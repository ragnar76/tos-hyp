struct cdrom_msf
{
    int8_t cdmsf_min0;       /* Start minute  */
    int8_t cdmsf_sec0;       /* Start second  */
    int8_t cdmsf_frame0;     /* Start frame   */
    int8_t cdmsf_min1;       /* End minute    */
    int8_t cdmsf_sec1;       /* End second    */
    int8_t cdmsf_frame1;     /* End frame     */
};
