!item [Binding:]
!begin_verbatim
int16_t pdlg_save_default_settings ( PRN_DIALOG *prn_dialog,
                                     PRN_SETTINGS *settings )
{
   addr_in[0] = prn_dialog;
   addr_in[1] = settings;

   int_in[0] = 10;

   crys_if (205);

   return ( int_out[0] );
}
