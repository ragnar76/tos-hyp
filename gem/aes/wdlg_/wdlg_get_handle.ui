int16_t wdlg_get_handle ( DIALOG *dialog );

!item [Binding:]
!begin_verbatim
int16_t wdlg_get_handle ( DIALOG *dialog )
{
   addr_in[0] = dialog;
   int_in[0] = 3;
   crys_if (164);

   return ( int_out[0] );
}
!end_verbatim

